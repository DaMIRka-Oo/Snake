<Window x:Class="SnakeEllipse.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SnakeEllipse"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="CanvasMap" HorizontalAlignment="Left" Height="390" VerticalAlignment="Top" Width="600" Loaded="CanvasMap_Loaded" Margin="100,10,0,0"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="390" VerticalAlignment="Top" Width="600" Margin="100,10,0,0"/>

    </Grid>
</Window>
